{
  "name": "eyp-tarball",
  "version": "1.0.0",
  "author": "Jordi Prats",
  "summary": "tarball deployment",
  "license": "Apache-2.0",
  "source": "https://github.com/SaltaIT/eyp-tarball",
  "project_page": "https://github.com/SaltaIT/eyp-vmwaretools",
